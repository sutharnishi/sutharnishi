			console.log("in Method?>>>>>>");
			var rows = tbody.rows;

			var subtotal = 0;

			for (var i = 0; i < rows.length; i++) {
				subtotal = subtotal + parseFloat(rows[i].cells[3].innerHTML);
				console.log("<<<<<<IN FORLOOP>>>>>>"+subtotal);
			}
			console.log("subtotal?>>>>>>"+subtotal);
			document.getElementById("grandtotal").innerHTML = subtotal;
			document.getElementById("sstotal").value = subtotal;

		}
	</script>



